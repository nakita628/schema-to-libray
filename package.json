{
  "name": "schema-to-library",
  "type": "module",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "pnpm -F @apps/generated dev",
    "deps": "rm -rf node_modules && rm -rf pnpm-lock.yaml && pnpm install",
    "schema-to-library-deps": "pnpm -F schema-to-library deps",
    "check": "pnpm biome check --write ./",
    "typecheck": "pnpm -F schema-to-library typecheck",
    "build": "pnpm -F schema-to-library build",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@types/node": "^24.2.0",
    "@vitest/coverage-v8": "^3.2.4",
    "vitest": "^3.2.4"
  }
}
